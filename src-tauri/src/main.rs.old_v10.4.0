#[tauri::command]
fn get_system_status(state: State<Arc<Mutex<TitaneCore>>>) -> Result<SystemStatus, String> {
    let core = state.lock().map_err(|e| format!("Lock error: {}", e))?;
    core.get_status().map_err(|e| e.to_string())
}

