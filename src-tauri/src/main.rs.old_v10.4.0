fn helios_get_metrics(state: State<Arc<Mutex<TitaneCore>>>) -> Result<String, String> {
    let core = state.lock().map_err(|e| format!("Lock error: {}", e))?;
    core.get_helios_metrics().map_err(|e| e.to_string())
}

