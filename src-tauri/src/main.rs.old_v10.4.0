#[tauri::command]
fn watchdog_get_logs(state: State<Arc<Mutex<TitaneCore>>>) -> Result<Vec<String>, String> {
    let core = state.lock().map_err(|e| format!("Lock error: {}", e))?;
    core.get_watchdog_logs().map_err(|e| e.to_string())
}
